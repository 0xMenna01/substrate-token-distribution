{
  "semi": false,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "jsxSingleQuote": false,
  "jsxBracketSameLine": false,
  "endOfLine": "auto",
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "vueIndentScriptAndStyle": true,
  "embeddedLanguageFormatting": "auto"
}
